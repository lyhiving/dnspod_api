{
    "name": "yakeing/dnspod_api",
    "description": "The DNSPod User API is restricted to individual users, making it easier and more flexible for users to manage their own domain names and records.",
    "keywords": ["DOMAIN", "SRV", "MX", "CNAME", "AAAA", "A", "TXT", "NS", "IP", "DNS"],
    "homepage": "https://github.com/yakeing/Dnspod_API",
    "type": "symfony-bundle",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "yakeing",
            "homepage": "http://weibo.com/yakeing"
        }
    ],
    "autoload": {
        "psr-0": {
            "dnspod_api\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
         "phpunit/phpunit": "5.7"
    },
    "extra":{
        "branch-alias":{
            "dev-master": "2.x-dev"
        }
    }
}
